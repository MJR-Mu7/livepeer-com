{
  "name": "@livepeer/player",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "@livepeer/apollo": "0.0.0",
    "@livepeer/graphql-sdk": "0.0.0",
    "@livepeer/sdk": "0.0.0",
    "apollo-cache-inmemory": "^1.1.1",
    "apollo-client": "^2.0.3",
    "apollo-link": "^1.0.3",
    "graphql": "git://github.com/graphql/graphql-js.git#npm",
    "graphql-tag": "^2.5.0",
    "query-string": "^5.0.1",
    "react": "^16.2.0",
    "react-apollo": "^2.0.1",
    "react-dom": "^16.2.0",
    "react-feather": "^1.0.7",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-scripts": "1.0.17",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "styled-components": "^2.2.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "PUBLIC_URL=https://livepeer.github.io/livepeerjs/ npm run build",
    "deploy": "npx gh-pages -d build",
    "prebuild": "cp webpack.config.prod.js ../../node_modules/react-scripts/config",
    "postbuild": "cp build/index.html build/404.html && npx uglify-es build/static/js/main.*.js -c -m -o build/static/js/$(ls build/static/js | head -n 1)",
    "serve": "npx serve -s build"
  },
  "devDependencies": {
    "gh-pages": "^1.1.0",
    "serve": "^6.4.1",
    "uglify-es": "^3.2.0"
  }
}
